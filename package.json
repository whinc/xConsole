{
  "name": "@whinc/xconsole",
  "version": "0.2.10",
  "repository": {
    "type": "git",
    "url": "https://github.com/whinc/xConsole.git"
  },
  "bugs": {
    "url": "https://github.com/whinc/xConsole/issues"
  },
  "main": "./dist/xconsole.min.js",
  "author": "whincwu",
  "license": "MIT",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "source-map-explorer": "^1.5.0",
    "babel-eslint": "^8.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "prop-types": "^15.6.0",
    "react-scripts": "1.0.16",
    "standard": "^10.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*",
    "copy:js": "cp ./build/static/js/main*.js ./dist/xconsole.min.js",
    "copy:css": "cp ./build/static/css/main*.css ./dist/xconsole.min.css",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "prepublish": "npm run build && npm run clean && npm run copy:js && npm run copy:css",
    "publish:major": "npm version major && npm publish --access=public",
    "publish:minor": "npm version minor && npm publish --access=public",
    "publish:patch": "npm version patch && npm publish --access=public"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "fetch",
      "URL",
      "it"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
